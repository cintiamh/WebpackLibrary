{
  "name": "library",
  "version": "0.0.1",
  "description": "A Webpack Babel Library Template",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --display-error-details && ./node_modules/.bin/webpack --prod",
    "dev": "./node_modules/.bin/webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "test": "node_modules/.bin/karma start karma.config.js",
    "test:watch": "ynpm run test -- --watch",
    "lint": "./node_modules/.bin/eslint . --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cintiamh/WebpackLibrary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cintiamh/WebpackLibrary/issues"
  },
  "homepage": "https://github.com/cintiamh/WebpackLibrary#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-istanbul": "^0.11.0",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3",
    "yargs": "^6.3.0"
  }
}
